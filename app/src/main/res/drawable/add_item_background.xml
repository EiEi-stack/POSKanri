<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/btn_add_start_color"></solid>
            <stroke android:width="1dp" android:color="@color/btn_add_stoke_color"></stroke>
            <corners android:radius="10dp"></corners>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="@color/btn_add_end_color" android:startColor="@color/btn_add_start_color"></gradient>
            <stroke android:width="1dp" android:color="@color/btn_add_stoke_color"></stroke>
            <corners android:radius="10dp"></corners>
        </shape>

    </item>
</selector>